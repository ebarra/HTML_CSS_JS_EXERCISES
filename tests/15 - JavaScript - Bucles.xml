      <text>Dadas las siguientes definiciones de funciones:
&lt;br&gt;&lt;br&gt;
// f_1 calcula potencias de 2. &lt;br&gt;
// El exponente es la parte entera de x. &lt;br&gt;&lt;br&gt;

function f_1 (x) { &lt;br&gt;
x = Math.floor(x); &lt;br&gt;
for (var res = 1; 0 &lt; x; x-- ) { &lt;br&gt;
res = res * 2; &lt;br&gt;
} &lt;br&gt;
return res; &lt;br&gt;
} &lt;br&gt;

&lt;br&gt;
// f_2 determina si parte entera de x es un número primo, &lt;br&gt;
// es decir si no es divisible por un número menor. &lt;br&gt;
&lt;br&gt;
function f_2 (x) { &lt;br&gt;
x = Math.floor(x); &lt;br&gt;
var i = 2; &lt;br&gt;
while ( i &lt; x ) { &lt;br&gt;
if ((x % i) === 0) { return false; }; &lt;br&gt;
i = i + 1; &lt;br&gt;
} &lt;br&gt;
return true; &lt;br&gt;
} &lt;br&gt;
